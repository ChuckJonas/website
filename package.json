{
  "name": "open-force",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "8.9.x"
  },
  "scripts": {
    "start": "node ./build/index.js",
    "dev-client": "cd react-app && npm start",
    "dev-server": "nodemon --exec ./node_modules/.bin/ts-node -- ./server/index.ts",
    "test": "npm run generate-json-schema && npm run validate-data",
    "generate-json-schema": "./node_modules/typescript-json-schema/bin/typescript-json-schema --required ./tsconfig.json AppData > ./data/app.schema.json",
    "validate-data": "node ./data/validator",
    "heroku-postbuild": "tsc & cd react-app/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "react-app/node_modules"
  ],
  "dependencies": {
    "@types/node": "^10.1.2",
    "@types/prop-types": "^15.5.6",
    "axios": "^0.18.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.3",
    "memory-cache": "^0.2.0",
    "tslib": "^1.9.3"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/memory-cache": "^0.2.0",
    "jsonschema": "^1.2.4",
    "nodemon": "^1.17.4",
    "ts-node": "^6.0.3",
    "typescript": "^2.8.1",
    "typescript-json-schema": "^0.32.0"
  }
}
